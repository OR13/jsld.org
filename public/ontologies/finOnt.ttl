<https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/RealEstate>
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/Valuation/hasEstimatedValue> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryAmount> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Places/Addresses/hasAddress> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/FND/Places/Addresses/PhysicalAddress> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/has> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/RealPropertyAppraisal> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isClassifiedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/ConstructionType>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isClassifiedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/DwellingCapacity>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isClassifiedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/ManufacturedHomeLegalClassification>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasNumberOfAffordableDwellingUnits> ;
    owl:someValuesFrom xsd:positiveInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasNumberOfDwellingUnits> ;
    owl:someValuesFrom xsd:positiveInteger
  ] .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/AllBorrowersMonthlyIncome>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryAmount>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FBC/DebtAndEquities/Debt/isBasedOn> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/BorrowerMonthlyIncome>
  ] ;
  rdfs:label "all borrowers monthly income" ;
  skos:definition "total monthly qualifying income for all borrowers on the loan" ;
  ns1:adaptedFrom "the 2015 Revised HMDA regulation." ;
  ns1:usageNote "This should be computed from the income of the individual borrowers." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/AmortizationSchedule>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/Schedule> ;
  rdfs:label "amortization schedule" ;
  skos:definition "a schedule which specifies changes in the scheduled balance of loan principal over time" ;
  skos:editorialNote "If needed, we can extend this to include the idea of an AmortizationScheduledPayment which includes interest and principal and many of which comprise an AmortizationSchedule" ;
  ns1:explanatoryNote "For simplicity, this definition assums that 'month n' ' on an AmortizationSchedule corresoponds to a CalculatedDate." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/AmortizationType>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "a category indicating how the loan amortizes" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/AutomatedUnderwritingSystem>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/AutomatedSystem> ;
  rdfs:label "automated underwriting system" ;
  skos:definition "a software sytstem that produces loan underwriting decisions" ;
  ns1:adaptedFrom "http://www.investopedia.com/terms/a/automated_underwriting.asp" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/BorrowerMonthlyIncome>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryAmount>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/hasPartyInRole> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FBC/DebtAndEquities/Debt/Borrower>
  ] ;
  rdfs:label "borrower monthly income" ;
  skos:definition "total monthly qualifying income of the potential borrower" ;
  ns1:adaptedFrom "the 2015 Revised HMDA regulation." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CashOutStatus>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "cash out status" ;
  skos:definition "a category indicating the extent to which funds are released to the borrower on a new loan origination that is  refinancing an existing loan" ;
  ns1:explanatoryNote "This is subject to lender and/or investor policy(s)." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/ClosedEndLoanContract>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/DebtInstrument>, <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/LoanContract>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasNoteAmount> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryAmount>
  ] ;
  rdfs:label "closed end loan contract" ;
  skos:definition "a loan contract that has an amount borrowed for a fixed amount of time, and is not considered open ended" ;
  ns1:adaptedFrom "2015 Revised HMDA regulation" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CombinedLTVRatioPercent>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/Percentage>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/hasRateValue> ;
    owl:allValuesFrom xsd:decimal
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/hasQuantityKind> ;
    owl:allValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/MonetaryMeasure>
  ] ;
  rdfs:label "combined loan to value ratio percent" ;
  skos:definition "the ratio of the total amount of debt that is secured by the property to the value of the property relied on." ;
  ns1:adaptedFrom "the 2015 Revised HMDA regulation." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToFund>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToPay> ;
  rdfs:label "commitment to fund" ;
  skos:definition "a commitment that obliges the lender to provide funds pursuant to a loan contract" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToPay>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/PaymentsAndSchedules/PaymentObligation>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/has> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/LineItem> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/hasPartyInRole> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/PaymentsAndSchedules/Payee>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/hasPartyInRole> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/PaymentsAndSchedules/Payer>
  ] ;
  rdfs:label "commitment to pay" ;
  skos:definition "a commitment obliging one party to make a payment to another party (e.g. to fund a loan, make payments, pay origination fees)" ;
  skos:editorialNote "Future consideration: perhaps add payment methods, and timing constraints." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToRepay>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToPay>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/has> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/PaymentHistory>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/has> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/PaymentSchedule>
  ] ;
  rdfs:label "commitment to repay" ;
  skos:definition "a commitment obliging the borrower to repay funds that have been loaned, in a manner pursuant to a loan contract" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CommitmentToReport>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Agreements/Commitment> ;
  rdfs:label "commitment to report" ;
  skos:definition "a commitment obliging a party to deliver a report" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/ConstructionType>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "construction type" ;
  skos:definition "a particular kind of construction" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditEvaluationProduct>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanExt/LoanGeneric/InformationProduct> ;
  rdfs:label "credit evaluation product" ;
  skos:definition "a particular kind of credit evaluation" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditInquiry>
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Class ;
    owl:intersectionOf (
     <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Communications/Request>
     _:genid21
   )
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Communications/isSentFrom> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/IndependentParty>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Communications/isSentTo> ;
    owl:someValuesFrom [
      a owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/playsRole> ;
      owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditRepository>
    ]
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasPartyBeingChecked> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/IndependentParty>
  ] ;
  rdfs:label "credit inquiry" ;
  skos:definition "a request from a lender to a credit repository to obtain information regarding a prospective borrower's creditworthiness" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditInquiryType>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "credit inquiry type" ;
  skos:definition "a category indicating whether a credit inquiry is a result of a borrower's direct authorization or by some indirect means" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditMessage>
  a owl:Class ;
  rdfs:subClassOf [
    a owl:Class ;
    owl:intersectionOf (
     <https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Values/TextValue>
     _:genid29
   )
  ] ;
  rdfs:label "credit message" ;
  skos:definition "a textual annotation on a credit report to notify a reviewer of special or exceptional information" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditMessageType>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "credit message type" ;
  skos:definition "a particular kind of credit message" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReport>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Reporting/Report>, [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditInquiry> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditScore> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditTradeLine> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises> ;
    owl:onClass <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/PublicRecord> ;
    owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/realizes> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReportProduct>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/isIdentifiedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/Identifier>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/hasDataSource> ;
    owl:someValuesFrom [
      a owl:Restriction ;
      owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/playsRole> ;
      owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditRepository>
    ]
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/hasAsOfDate> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/Date>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/comprises> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditMessage>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isClassifiedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReportCategory>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/isProducedBy> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReportingAgency>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasCoveragePeriod> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/DatePeriod>
  ], [
    a owl:Restriction ;
    owl:onProperty <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/hasPartyBeingChecked> ;
    owl:someValuesFrom <https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/IndependentParty>
  ] ;
  rdfs:label "credit report" ;
  skos:definition "a report describing the creditworthiness and related credit attributes of a borrower." ;
  ns1:explanatoryNote "This is typically provided by a credit reporting agency but could be produced by an internal proprietary model as well." .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReportCategory>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/Classifier> ;
  rdfs:label "credit report category" ;
  skos:definition "a particular kind of credit report, often available from multiple vendors" .

<https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditReportProduct>
  a owl:Class ;
  rdfs:subClassOf <https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanCore/CreditEvaluationProduct> ;
